-- Destroys UI 
for i,v in pairs(game.CoreGui:GetDescendants()) do
    if v.Name == "SchoolHub" then
    v:Destroy()
    end
end

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lolpoppyus/Roblox-Lua/master/Pop%20UI%20Lib", true))()

local ui = library:Tab("SchoolHub Selector")

ui:Button("SchoolHub Pc / Emulator", function()
    print("yes")
end)

ui:Button("SchoolHub Mobile (Coming Soon)", function()
    print("yes")
end)

ui:Button("Destroy GUI", function()
    for i, v in pairs(game.CoreGui:GetChildren()) do
        if v:FindFirstChild("SchoolHub") then
            v:Destroy()
        end
    end 
end)



